{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "Kafka": {
        "BootstrapServers": "kafka:29092"
    },
    "RabbitMQ": {
        "Uri": "amqp://guest:guest@rabbitmq:5672/"
    },
    "Database": {
        "Provider": "SqlServer"
    },
    "Sqlite_Database": "Data Source=SPOSM_client_{clientId}.db",
    "SqlServer_Database": "Server=localhost;Database=SPOSM_client_{clientId};Trusted_Connection=True;TrustServerCertificate=True;",
    "Testing": {
        "Database": {
            "Provider": "SqlServer"
        },
        "Sqlite_Database": "Data Source=SQLITE_TEST.db",
        "SqlServer_Database": "Server=localhost;Database=testing_product_ordering_service;Trusted_Connection=True;TrustServerCertificate=True;"
    }
}
